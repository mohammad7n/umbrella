{
    "env": {
        "node": true,
        "es6": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "ignorePatterns": [
        "package-lock.json",
        "package.json",
        "node_modules/*"
    ],
    "rules": {
        "default-case-last": "error",
        "default-param-last": "error",
        "dot-location": [
            "error",
            "property"
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-caller": "error",
        "no-else-return": "error",
        "no-eval": "error",
        "no-fallthrough": "error",
        "no-implied-eval": "error",
        "no-invalid-this": "error",
        "no-labels": "error",
        "semi": [
            "error",
            "always"
        ],
        "no-proto": "error",
        "no-redeclare": "error",
        "no-self-assign": "error",
        "no-self-compare": "error",
        "no-useless-escape": "error",
        "no-void": "error",
        "no-with": "error",
        "yoda": [
            "error",
            "never"
        ],
        "init-declarations": [
            "error",
            "always"
        ],
        "no-delete-var": "error",
        "no-use-before-define": "error",
        "func-call-spacing": [
            "error",
            "never"
        ],
        "new-parens": "error",
        "no-confusing-arrow": "error",
        "no-const-assign": "error",
        "no-duplicate-imports": "error",
        "no-var": "error",
        "prefer-rest-params": "error",
        "require-yield": "error",
        "rest-spread-spacing": [
            "warn",
            "never"
        ],
        "prefer-const": "warn",
        "prefer-exponentiation-operator": "warn",
        "prefer-object-spread": "warn",
        "object-curly-spacing": [
            "warn",
            "always"
        ],
        "prefer-destructuring": [
            "warn",
            {
                "array": true,
                "object": true
            },
            {
                "enforceForRenamedProperties": false
            }
        ],
        "no-trailing-spaces": "warn",
        "no-whitespace-before-property": "error",
        "array-bracket-spacing": [
            "warn",
            "never"
        ],
        "arrow-spacing": [
            "warn",
            {
                "before": true,
                "after": true
            }
        ],
        "template-tag-spacing": "warn",
        "switch-colon-spacing": [
            "warn",
            {
                "before": false,
                "after": true
            }
        ],
        "spaced-comment": [
            "warn",
            "always"
        ],
        "require-await": "warn",
        "no-useless-return": "warn",
        "no-unmodified-loop-condition": "warn",
        "no-return-await": "warn",
        "no-multi-spaces": "warn",
        "no-magic-numbers": "warn",
        "space-in-parens": [
            "warn",
            "never"
        ],
        "semi-spacing": [
            "warn",
            {
                "before": false,
                "after": true
            }
        ],
        "block-spacing": [
            "warn",
            "always"
        ],
        "comma-spacing": [
            "warn",
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            "warn",
            "last"
        ],
        "key-spacing": [
            "warn",
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "quotes": [
            "warn",
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "indent": [
            "warn",
            4
        ],
        "no-mixed-spaces-and-tabs": "warn",
        "max-len": [
            "warn",
            {
                "code": 120,
                "comments": 120
            }
        ],
        "brace-style": [
            "warn",
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": [
            "warn",
            {
                "properties": "never"
            }
        ],
        "eol-last": 2
    }
}